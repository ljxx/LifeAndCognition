<?xml version="1.0" encoding="utf-8"?>
<com.ylixiang.ylxcommonlib.view.YCardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="@dimen/qb_px_6">

    <ImageView
        android:id="@+id/m_img_iv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_launcher_background"/>

    <TextView
        android:id="@+id/m_title_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/qb_px_10"
        android:visibility="gone"
        android:ellipsize="end"/>

</com.ylixiang.ylxcommonlib.view.YCardView>
